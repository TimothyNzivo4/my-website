{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": {} }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
